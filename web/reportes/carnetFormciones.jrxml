	<queryString>
		<![CDATA[SELECT 
    e.NOMBRES, 
    e.APELLIDOS,
    e.CEDULA,
    e.FOTOGRAFIA,
    e.VENCE_CARNET,
    e.IDENTIFICADOR_UNICO, 
    td.NOMBRE AS TIPO_DOCUMENTO, 
    f.NOMBRE AS FORMACION, 
    f.ID_FORMACION AS NUMERO_FICHA, 
    s.NOMBRE AS SEDE,
    logo.IDLOGO,
    logo.IMAGEN,
    logo.DIRECTOR, 
    logo.FIRMA
FROM 
    estudiantes e
INNER JOIN 
    formacion f ON e.FORMACION_FK = f.ID_FORMACION
INNER JOIN 
    sede s ON f.SEDE_ID = s.ID_SEDE
INNER JOIN 
    tipodocumento td ON e.TIPO_DOCUMENTO_FK = td.ID_TIPO_DOCUMENTO
CROSS JOIN 
    (SELECT IDLOGO ,DIRECTOR, IMAGEN , FIRMA FROM logo LIMIT 1) AS logo
WHERE 
    f.ID_FORMACION = $P{id}]]>
	</queryString>
